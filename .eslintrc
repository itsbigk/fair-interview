{
  parser: "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [1, 120, 2, {ignoreComments: true}]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "fetch": false,
    "localStorage": false,
    "window": false,
    "URLSearchParams": false,
    "document": false,
    "module": false
  }
}
